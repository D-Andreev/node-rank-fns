{
    "main": "lib/binding.js",
    "name": "node-rank-fns",
    "version": "1.0.0",
    "binary": {
        "napi_versions": [
            3
        ]
    },
    "scripts": {
        "install": "prebuild-install --runtime napi || cmake-js build",
        "rebuild": "cmake-js build",
        "prebuild": "prebuild --runtime napi --all --strip --verbose",
        "upload": "prebuild --runtime napi --upload ${GITHUB_TOKEN}",
        "pretest": "tsc",
        "test": "node --napi-modules test/index.js"
    },
    "author": "Dimitar Andreev",
    "license": "MIT",
    "description": "Nodejs addon with ranking functions",
    "dependencies": {
        "bindings": "^1.5.0",
        "node-addon-api": "^3.2.0",
        "prebuild-install": "^6.1.2"
    },
    "devDependencies": {
        "@types/node": "^7.0.22",
        "cmake-js": "^6.1.0",
        "typescript": "^3.9.7"
    },
    "repository": "https://github.com/D-Andreev/node-rank-fns.git"
}
