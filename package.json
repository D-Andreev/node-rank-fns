{
    "main": "lib/binding.js",
    "name": "recommender-system",
    "version": "1.0.0",
    "binary": {
        "napi_versions": [
            3
        ]
    },
    "scripts": {
        "install": "prebuild-install --runtime napi || cmake-js build",
        "rebuild": "cmake-js build",
        "prebuild": "prebuild --runtime napi --all --strip --verbose",
        "upload": "prebuild --runtime napi --upload ${GITHUB_TOKEN}",
        "pretest": "tsc",
        "test": "npm run rebuild && cd build && ctest",
        "test:feature": "node --napi-modules test/index.js",
        "benchmarks": "node --napi-modules benchmarks/index.js"
    },
    "author": "Dimitar Andreev",
    "license": "MIT",
    "description": "A native node addon with implementations of various utility functions commonly used in recommender systems.",
    "dependencies": {
        "bindings": "^1.5.0",
        "node-addon-api": "^3.2.0",
        "prebuild-install": "^6.1.2"
    },
    "devDependencies": {
        "@types/node": "^7.0.22",
        "cmake-js": "^6.1.0",
        "fastbench": "^1.0.1",
        "typescript": "^3.9.7"
    },
    "repository": "https://github.com/D-Andreev/recommender-system.git"
}
